{
  "stack": "dotnet",
  "memory": 256,
  "runtimeArgs": [
  ],
  "routes": {
    "getRates": {
      "path": "/shp-rates/calculate",
      "settingsType": "workspace",
      "public": false,
      "access": "authorized",
      "policies": [
        {
          "effect": "allow",
          "actions": [
            "get","post"
          ],
          "principals": [
            "vrn:apps:*:{{account}}:{{workspace}}:app/vtex.shipping-rates-provider@*"
          ]
        }
      ]
    },
    "getRawRates": {
      "path": "/shipping-utilities/getRawRates/:carrier",
      "public": true
    },
    "track": {
      "path": "/shipping-utilities/track/:carrier/:trackingNumber",
      "public": true
    },
    "getMerchantSettings": {
      "path": "/shipping-utilities/getSettings/:carrier",
      "public": true
    },
    "setMerchantSettings": {
      "path": "/shipping-utilities/setSettings/:carrier",
      "public": true
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": ["linked"]
    }
  }
}
