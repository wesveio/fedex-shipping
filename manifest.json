{
  "name": "fedex-shipping",
  "vendor": "vtexus",
  "version": "1.17.2",
  "title": "FedEx Shipping",
  "description": "An app for interfacing with FedEx to provide rates and tracking information.",
  "scripts": {
    "prereleasy": "bash pre.sh"
  },
  "billingOptions": {
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.apps-graphql": "2.x"
  },
  "builders": {
    "dotnet": "2.x",
    "vtex.shipping-rates-provider": "0.x",
    "messages": "1.x",
    "admin": "0.x",
    "react": "3.x",
    "docs": "0.x",
    "graphql": "1.x"
  },
  "policies": [
    {
      "name": "graphql-query"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "bnb.data.bl.uk"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "nuget.org"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercebeta.com.br",
        "path": "/api/logistics/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/logistics/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{workspace}}--{{account}}.myvtex.com",
        "path": "/packAll"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "infra.io.vtex.com",
        "path": "/vbase/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "infra.io.vtex.com",
        "path": "/apps/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "wsbeta.fedex.com",
        "path": "/web-services/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ws.fedex.com",
        "path": "/web-services/*"
      }
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
